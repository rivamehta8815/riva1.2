#include<stdio.h>
int main()
{
int a,b,add,sub,prod,divi;
printf("enter the values a,b");
scanf("%d%d",&a,&b);
add=a+b;
printf(" sum of two numbers is %d\n",add);
prod=a*b;
printf(" product of two numbers is %d\n",prod);
sub=a-b;
printf("difference of two numbers is %d\n",sub);
divi=a/b;
printf(" quotient of two numbers is %d\n",divi);
return 0;
}